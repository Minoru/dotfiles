[user]
    name = Alexander Batischev
    email = eual.jp@gmail.com
[merge]
    tool = vimdiff
[diff]
    renames = copy
    algorithm = patience
[push]
    default = upstream
[core]
    quotepath = false
[branch]
    autosetuprebase = always
[color]
    branch = auto
    diff = auto
    status = auto
[alias]
    st = status
    di = diff
    ci = commit
    co = checkout
    lg = log --abbrev-commit --graph --all --decorate --oneline
[pull]
    rebase = preserve
[grep]
    lineNumber = true
[gpg]
	program = gpg2
[protocol]
	version = 2
